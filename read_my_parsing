le parsing de doom

sizeof(long) : balise "ğŸ’ğŸ‡©ğŸ‡¿ğŸ‰ğŸ’©"
sizeof(int) : balise (Texture) "ğŸŒ…"
sizeof(int) : number of textures in map (currently number loaded in editor)
foreach {
	sizeof(char) * strlen(path de texture) : Path de la Texture
	sizeof(char) : '\v'
}
sizeof(int) : balise (End Texture) "ğŸŒŒ"
sizeof(int) : balise (Pillars) "â›©"
sizeof(int) : number of Pillars
foreach {
	sizeof(double) x pos
	sizeof(double) y pos
}
sizeof(int) : balise (End Pillars) "ğŸ’Š"
sizeof(int) : balise (Sectors) "ğŸš§"
sizeof(int) : number of Sectors
foreach {
	sizeof(char) : Gravity type (0 : Moon | 1 : Earth)
	sizeof(int) : Hauteur sol par rapport a 0
	sizeof(int) : Hauteur plafond par rapport a sol
	sizeof(int) : ID Texture Sol (Must be less than number of textures in map)
	sizeof(int) : ID Texture Plafond (Must be less than number of textures in map)
	sizeof(int) : balise (Walls) "ğŸ’¦"
	sizeof(int) : number of Walls
	foreach {
		sizeof(int) : Pillar start ID (Must be less than number of Pillars)
		sizeof(int) : Pillar end ID (Must be less than number of Pillars)
		sizeof(int) : Texture ID (Must be less than number of textures in map)
		sizeof(int) : Portal type (Enum e_portal_id)
		sizeof(int) : Sector ID (Ignored if Portal type is 0) (Must still be read)
		sizeof(int) : balise (Wall Props) "ğŸ–¼"
		sizeof(int) : number of Wall Props
		foreach {
			sizeof(int) : type
			sizeof(int) : Sector ID (Must be less than number of Sector)
			sizeof(int) : Wall ID (of current sector) (Must be less than number of Walls in Sector)
			sizeof(double) : pos x
			sizeof(double) : pos y
		}
		sizeof(int) : balise (End Wall Props) "ğŸ“…"
	}
	sizeof(int) : balise (End Walls) "â›±"

	sizeof(int) : balise (Props) "ğŸš½"
	sizeof(int) : number of Props
	foreach {
		sizeof(int) : type
		sizeof(int) : Sector ID (Must be less than number of Sector)
		sizeof(int) : Wall ID (of current sector) (Must be less than number of Walls in Sector)
		sizeof(double) : pos x
		sizeof(double) : pos y
	}
	sizeof(int) : balise (End Props) "ğŸ’©"
}
sizeof(int) : balise (End Sectors) "ğŸ "
sizeof(int) : balise (Player) "ğŸ†"
	sizeof(int) : Sector ID (Must be less than number of Sector)
	sizeof(int) : Max Health
	sizeof(double) : pos x
	sizeof(double) : pos y
	sizeof(double) : rotation y
sizeof(int) : balise (End Player) "ğŸŒ"
sizeof(int) : balise (Enemy) "ğŸ”ª"
sizeof(int) : number of Enemy
foreach {
	sizeof(int) : type
	sizeof(int) : Sector ID (Must be less than number of Sector)
	sizeof(double) : pos x
	sizeof(double) : pos y
	sizeof(double) : rotation y
}
sizeof(int) : balise (End Enemy) "ğŸ¹"
sizeof(long) : balise "ğŸ‘¨ğŸ»ğŸ¤ ğŸ‘"
